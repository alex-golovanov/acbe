{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "module": "ES2020",
    "moduleResolution": "node",
    "noEmit": true,
    "paths": {
      "ajaxes": [ "src/dualUse/ajaxes/index.ts" ],
      "ajaxes/*": [ "src/dualUse/ajaxes/*" ],
      "alarms": [ "src/general/alarms/index.ts" ],
      "alarms/*": [ "src/general/alarms/*" ],
      "availableServer": [ "src/dualUse/availableServer/index.ts" ],
      "availableServer/*": [ "src/dualUse/availableServer/*" ],
      "bg/*": [ "src/bg/*" ],
      "config": [ "config/production.d.ts" ],
      "DelayRecord": [ "src/dualUse/DelayRecord.js" ],
      "ga": [ "src/dualUse/ga/index.ts" ],
      "ga/full": [ "src/dualUse/ga/full/chrome/index.ts" ],
      "ga/partial": [ "src/dualUse/ga/partial/chrome/index.ts" ],
			"globalIgnoredDomains": [ "src/general/globalIgnoredDomains.ts" ],
			"experiments": [ "src/general/experiments/index.ts" ],
      "highLevelPac": [ "src/dualUse/highLevelPac/index.ts" ],
      "highLevelPac/*": [ "src/dualUse/highLevelPac/*" ],
      "jitsu": [ "src/dualUse/jitsu.ts" ],
      "LocalDelayRecord": [ "src/general/LocalDelayRecord.js" ],
      "log": [ "src/dualUse/log/index.ts" ],
      "log/*": [ "src/dualUse/log/*" ],
      "lowLevelPac": [ "src/dualUse/lowLevelPac/index.ts" ],
      "lowLevelPac/*": [ "src/dualUse/lowLevelPac/*" ],
      "tools/*": [ "src/general/tools/*" ],
      "22pac/createPac": [ "src/dualUse/pac/chrome/index.js" ],
      "pageLinks": [ "src/general/pageLinks.ts" ],
      "polyfills/*": [ "src/polyfills/*" ],
      "proxy": [ "src/dualUse/proxy/index.ts" ],
      "servers.json": [ "src/servers.json" ],
      "store": [ "src/general/store/index.ts" ],
      "storage": [ "src/general/storage.ts" ],
      "time": [ "src/general/time.ts" ],
      "timezoneChange/*": [ "src/timezoneChange/*" ],
      "userProperties": [ "src/dualUse/userProperties.ts" ],
      "utils": [ "src/utils/index.ts" ],
      "images": [ "src/images/index.ts" ],
      "constants/*": [ "src/constants/*" ],
      "general/*": [ "src/general/*" ],
      "popup/*": [ "src/popup/*" ],
      "contentScripts/*": [ "src/contentScripts/*" ],
      "types/*": [ "types/*" ]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "ESNext",
    "useUnknownInCatchVariables": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
  },
  "exclude": [
    "**/*.spec.ts",
    "flow-typed",
    "node_modules",
    "src/timezoneChange/dateCodeChange.js",
    "target"
  ]
}