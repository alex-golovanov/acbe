  "scripts": {
    "build": "npm run lint && gulp",
    "build:dev": "ENV=development npm run build",
    "build:qa": "ENV=qa npm run build",
    "gulp": "gulp",
    "flow": "flow src --show-all-errors",
    "lint": "eslint --quiet --ext .js,.ts build config selenium src selenium.js",
    "lint:fix": "npm run lint -- --fix",
    "originalflow": "flow",
    "package": "npm run lint && node ./build/npm/package.js",
    "selenium": "node selenium.js",
    "ts": "tsc -p tsconfig.json",
    "test": "jest --no-cache",
    "test:domains": "jest --watch --verbose --no-cache --runInBand --testPathPattern=domains",
    "test:pac": "jest --watch --verbose --no-cache --runInBand --testPathPattern=pac",
    "validate": "addons-linter --log-level=error target/",
    "postversion": "npm run gulp downloaddomaindependencies && npm run gulp servers && git add . && git commit -m \"Update domain dependencies and servers\" || echo \"No changes to commit\""
  },
  "repository": {
    "type": "git",
